{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2023"],
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "Node16",
      "sourceMap": true,
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictNullChecks": false,
      "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
